{
  "version": 2,
  "builds": [
    {
      "src": "src/server/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server/main.ts"
    },
    {
      "src":"/cron",
      "dest":"src/server/cron/index.ts"
    }
  ],
  "crons":[
    {"path":"/cron",
     "schedule":"0 */24 * * *"
  }
  ]
}
